<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="event_twitter_wall_view" inherit_id="website_twitter_wall.twitter_wall_view" name="Event Twitter Wall" primary="True">
    <xpath expr="t[@t-call='website.layout']" position="attributes">
        <attribute name="t-call">website_event.layout</attribute>
    </xpath>
    <xpath expr="//div[hasclass('o-tw-options')]//div" position="attributes">
        <attribute name="groups">base.group_user</attribute>
    </xpath>
    <xpath expr="//div[hasclass('o-tw-walls-col')]" position="attributes">
        <attribute name="class">o-tw-walls-col col-md-4</attribute>
    </xpath>
</template>

</odoo>
